{
  "name": "react",
  "version": "0.0.0",
  "private": true,
  "description": "A boilerplate for react.",
  "scripts": {
    "build": "vite build",
    "check:deps": "ncu",
    "check:types": "tsc --noEmit",
    "commit": "cz",
    "dev": "vite",
    "lint": "run-p lint:eslint lint:markdownlint lint:stylelint check:types",
    "lint:eslint": "eslint . --fix --ext=.js,.cjs,.mjs,.jsx,.ts,.cts,.mts,.tsx --ignore-path=.gitignore --cache --cache-location=node_modules/.vite/vite-plugin-eslint",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "lint:stylelint": "stylelint \"./**/*.{css,less,scss,sass}\" --fix --allow-empty-input --ignore-path=.gitignore --cache --cache-location=node_modules/.vite/vite-plugin-stylelint",
    "prepare": "is-ci || husky install",
    "release": "run-s release-it build",
    "release-it": "release-it",
    "preview": "vite preview"
  },
  "dependencies": {
    "@ant-design/colors": "^6.0.0",
    "@ant-design/icons": "^4.7.0",
    "@iconify/react": "^3.2.2",
    "@tanstack/react-query": "^4.0.10",
    "ahooks": "^3.6.2",
    "antd": "^4.22.3",
    "axios": "^0.27.2",
    "browser-update": "^3.3.40",
    "change-case": "^4.1.2",
    "clsx": "^1.2.1",
    "color": "^4.2.3",
    "dayjs": "^1.11.4",
    "immer": "^9.0.15",
    "lodash-es": "^4.17.21",
    "modern-normalize": "^1.1.0",
    "nprogress": "^0.2.0",
    "qs": "^6.11.0",
    "ramda": "^0.28.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "react-use": "^17.4.0",
    "statuses": "^2.0.1",
    "usehooks-ts": "^2.6.0",
    "zustand": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/eslint-parser": "^7.18.9",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@commitlint/prompt": "^17.0.3",
    "@iconify/json": "^2.1.87",
    "@modyqyw/fabric": "^5.6.0",
    "@modyqyw/tailwind-presets": "^0.12.1",
    "@modyqyw/vite-plugin-eslint": "^2.0.8",
    "@release-it/conventional-changelog": "^5.0.0",
    "@svgr/core": "^6.3.1",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.4",
    "@types/browser-update": "^3.3.0",
    "@types/color": "^3.0.3",
    "@types/lodash-es": "^4.17.6",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7",
    "@types/ramda": "^0.28.15",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/statuses": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "@vitejs/plugin-react": "^2.0.0",
    "commitizen": "^4.2.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.21.0",
