    "npm-check-updates": "^16.0.5",
    "npm-run-all": "^4.1.5",
    "picocolors": "^1.0.0",
    "postcss": "^8.4.14",
    "postcss-preset-env": "^7.7.2",
    "prettier": "^2.7.1",
    "release-it": "^15.2.0",
    "rimraf": "^3.0.2",
    "sass": "^1.54.2",
    "shelljs": "^0.8.5",
    "stylelint": "^14.9.1",
    "tailwindcss": "^3.1.7",
    "type-fest": "^2.18.0",
    "typescript": "^4.7.4",
    "unplugin-auto-import": "^0.10.3",
    "unplugin-icons": "^0.14.8",
    "vite": "^3.0.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-env-compatible": "^1.1.1",
    "vite-plugin-inspect": "^0.6.0",
