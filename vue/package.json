    "vite-plugin-compression2": "^0.10.5",
    "vite-plugin-eslint2": "^4.1.0",
    "vite-plugin-inspect": "^0.7.40",
    "vite-plugin-stylelint": "^5.1.1",
    "vite-plugin-vue-devtools": "^0.5.1",
    "vite-plugin-vue-meta-layouts": "^0.3.1",
    "vite-plugin-warmup": "^0.1.0",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.19"
  },
  "packageManager": "pnpm@8.9.2",
  "engines": {
    "node": "18"
  }
}
