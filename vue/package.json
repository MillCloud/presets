{
  "name": "vue",
  "version": "0.0.0",
  "private": true,
  "description": "A boilerplate for vue3.",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "check:deps": "ncu",
    "check:types": "vue-tsc --noEmit",
    "commit": "cz",
    "dev": "vite",
    "lint": "run-p lint:eslint lint:markdownlint lint:stylelint check:types",
    "lint:eslint": "eslint . --fix --ext=.js,.cjs,.mjs,.jsx,.ts,.cts,.mts,.tsx,.vue --ignore-path=.gitignore --cache --cache-location=node_modules/.vite/vite-plugin-eslint",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "lint:stylelint": "stylelint \"./**/*.{css,less,scss,sass,vue}\" --fix --allow-empty-input --ignore-path=.gitignore --cache --cache-location=node_modules/.vite/vite-plugin-stylelint",
    "prepare": "is-ci || husky install",
    "release": "run-s release-it build",
    "release-it": "release-it",
    "preview": "vite preview",
    "test:unit": "vitest run --coverage"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.10",
    "@iconify/vue": "^4.0.0",
    "@lukemorales/query-key-factory": "^1.0.0",
    "@tanstack/vue-query": "^4.13.5",
    "@vueuse/core": "^9.4.0",
    "@vueuse/router": "^9.3.0",
    "axios": "^1.1.3",
    "browser-update": "^3.3.44",
    "change-case": "^4.1.2",
    "color": "^4.2.3",
    "dayjs": "^1.11.6",
    "element-plus": "^2.2.19",
    "element-pro-components": "^1.2.3",
    "lodash-es": "^4.17.21",
    "modern-normalize": "^1.1.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.23",
    "qs": "^6.11.0",
    "ramda": "^0.28.0",
    "statuses": "^2.0.1",
    "vue": "^3.2.41",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/eslint-parser": "^7.19.1",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@commitlint/prompt": "^17.1.2",
    "@iconify/json": "^2.1.131",
    "@modyqyw/fabric": "^5.11.0",
    "@modyqyw/vite-plugin-eslint": "^2.0.10",
    "@release-it/conventional-changelog": "^5.1.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.7",
    "@types/browser-update": "^3.3.0",
    "@types/color": "^3.0.3",
    "@types/jsdom": "^20.0.0",
    "@types/lodash-es": "^4.17.6",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7",
    "@types/ramda": "^0.28.18",
    "@types/shelljs": "^0.8.11",
    "@types/statuses": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "@vitejs/plugin-legacy": "^2.3.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitejs/plugin-vue-jsx": "^2.1.0",
    "@vitest/coverage-c8": "^0.24.4",
    "@vitest/ui": "^0.24.4",
    "@vue/test-utils": "^2.2.1",
    "@vue/tsconfig": "^0.1.3",
    "c8": "^7.12.0",
    "commitizen": "^4.2.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.26.0",
    "happy-dom": "^7.6.6",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "jsdom": "^20.0.2",
    "lint-staged": "^13.0.3",
    "markdownlint-cli": "^0.32.2",
    "npm-check-updates": "^16.3.16",
    "npm-run-all": "^4.1.5",
    "picocolors": "^1.0.0",
    "postcss": "^8.4.18",
    "postcss-preset-env": "^7.8.2",
    "prettier": "^2.7.1",
    "release-it": "^15.5.0",
    "rimraf": "^3.0.2",
    "sass": "^1.55.0",
    "shelljs": "^0.8.5",
    "stylelint": "^14.14.0",
    "tailwind-extensions": "^0.18.0",
    "tailwindcss": "^3.2.1",
    "terser": "^5.15.1",
    "tsx": "^3.11.0",
    "type-fest": "^3.1.0",
    "typescript": "^4.8.4",
    "unplugin-auto-import": "^0.11.4",
    "unplugin-icons": "^0.14.12",
    "unplugin-vue-components": "^0.22.9",
    "unplugin-vue-macros": "^0.15.1",
    "vite": "^3.2.2",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-inspect": "^0.7.5",
    "vite-plugin-mkcert": "^1.10.1",
    "vite-plugin-pages": "^0.27.1",
    "vite-plugin-progress": "^0.0.6",
    "vite-plugin-stylelint": "^3.0.8",
    "vite-plugin-time-reporter": "^2.0.0",
    "vite-plugin-vue-layouts": "^0.7.0",
    "vitest": "^0.24.3",
    "vue-tsc": "^1.0.9"
  }
}
