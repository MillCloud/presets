{
  "name": "vue",
  "version": "0.0.0",
  "private": true,
  "description": "A boilerplate for vue3.",
  "type": "module",
  "scripts": {
    "build": "cross-env VITE_MODE=production vite build",
    "build:dev": "cross-env VITE_MODE=development vite build",
    "build:prod": "pnpm run build",
    "build:staging": "cross-env VITE_MODE=staging vite build",
    "check:deps": "taze -f",
    "check:types": "vue-tsc --noEmit",
    "commit": "git-cz",
    "dev": "cross-env VITE_MODE=development vite",
    "dev:dev": "pnpm run dev",
    "dev:prod": "cross-env VITE_MODE=production vite",
    "dev:staging": "cross-env VITE_MODE=staging vite",
    "lint": "conc \"pnpm:lint:eslint\" \"pnpm:lint:markdownlint\" \"pnpm:lint:stylelint\"",
    "lint:eslint": "eslint . --fix --cache --ignore-path=.gitignore",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "lint:stylelint": "stylelint \"./**/*.{css,scss,vue}\" --fix --cache --ignore-path=.gitignore",
    "prepare": "is-ci || simple-git-hooks",
    "preview": "vite preview",
    "release": "commit-and-tag-version -a",
    "test": "vitest run --coverage --passWithNoTests"
  },
  "dependencies": {
    "@ctrl/tinycolor": "^3.6.0",
    "@iconify/vue": "^4.1.1",
    "@modyqyw/utils": "^8.0.0",
    "@tanstack/vue-query": "^4.33.1",
    "@vueuse/core": "^10.3.0",
    "@vueuse/router": "^10.3.0",
    "axios": "^1.4.0",
    "browser-update": "^3.3.44",
    "change-case": "^4.1.2",
    "dayjs": "^1.11.9",
    "modern-normalize": "^2.0.0",
    "naive-ui": "^2.34.4",
    "naive-ui-pro-components": "0.3.0-beta.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.6",
    "qs": "^6.11.2",
    "statuses": "^2.0.1",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@commitlint/prompt": "^17.7.1",
    "@iconify/json": "^2.2.103",
    "@modyqyw/fabric": "^9.0.3",
    "@types/browser-update": "^3.3.0",
    "@types/eslint": "^8.44.2",
    "@types/node": "^18.17.8",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7",
    "@types/shelljs": "^0.8.12",
    "@types/statuses": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@unocss/reset": "^0.55.2",
    "@vitejs/plugin-legacy": "^4.1.1",
    "@vitejs/plugin-vue": "^4.3.3",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vitest/coverage-v8": "^0.34.2",
    "@vitest/ui": "^0.34.2",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "commitizen": "^4.3.0",
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.47.0",
    "is-ci": "^3.0.1",
    "lint-staged": "^14.0.1",
    "markdownlint-cli": "^0.35.0",
    "picocolors": "^1.0.0",
    "postcss": "^8.4.28",
    "postcss-preset-env": "^9.1.1",
    "prettier": "^3.0.2",
    "rimraf": "^5.0.1",
    "sass": "^1.66.1",
    "shelljs": "^0.8.5",
    "simple-git": "^3.19.1",
    "simple-git-hooks": "^2.9.0",
    "sort-package-json": "^2.5.1",
    "stylelint": "^15.10.3",
    "taze": "^0.11.2",
    "terser": "^5.19.2",
    "tsx": "^3.12.7",
    "type-fest": "^3.13.1",
    "typescript": "~5.0.4",
    "unocss": "^0.55.2",
    "unocss-preset-naive-ui": "^0.0.2",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-icons": "^0.16.5",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.4.9",
    "vite-plugin-checker": "^0.6.2",
    "vite-plugin-compression2": "^0.10.4",
    "vite-plugin-eslint2": "^4.1.0",
    "vite-plugin-inspect": "^0.7.38",
    "vite-plugin-pages": "^0.31.0",
    "vite-plugin-stylelint": "^5.1.0",
    "vite-plugin-vue-devtools": "^0.5.1",
    "vite-plugin-vue-layouts": "^0.8.0",
    "vite-plugin-warmup": "^0.1.0",
    "vitest": "^0.34.2",
    "vue-tsc": "^1.8.8"
  },
  "packageManager": "pnpm@8.6.12",
  "engines": {
    "node": "18"
  }
}
