{
  "name": "vue",
  "version": "0.0.0",
  "private": true,
  "description": "A boilerplate for vue3.",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "check:deps": "ncu --dep dev,optional,prod,packageManager",
    "check:types": "vue-tsc --noEmit",
    "commit": "git-cz",
    "dev": "vite",
    "lint": "run-p lint:eslint lint:markdownlint lint:stylelint check:types",
    "lint:eslint": "eslint .  --fix --cache --no-error-on-unmatched-pattern",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "lint:stylelint": "stylelint \"./**/*.{css,scss,vue}\" --fix --cache --allow-empty-input --ignore-path=.gitignore",
    "prepare": "is-ci || husky install",
    "release": "run-s release-it build",
    "release-it": "release-it",
    "preview": "vite preview",
    "test:unit": "vitest run --coverage"
  },
  "dependencies": {
    "@ctrl/tinycolor": "^3.5.0",
    "@element-plus/icons-vue": "^2.0.10",
    "@iconify/vue": "^4.1.0",
    "@lukemorales/query-key-factory": "^1.1.0",
    "@tanstack/vue-query": "^4.24.4",
    "@vueuse/core": "^9.12.0",
    "@vueuse/router": "^9.12.0",
    "axios": "^1.3.0",
    "browser-update": "^3.3.44",
    "change-case": "^4.1.2",
    "dayjs": "^1.11.7",
    "element-plus": "^2.2.28",
    "element-pro-components": "^1.2.9",
    "modern-normalize": "^1.1.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.29",
    "qs": "^6.11.0",
    "statuses": "^2.0.1",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@commitlint/prompt": "^17.4.2",
    "@iconify/json": "^2.2.14",
    "@modyqyw/fabric": "^6.0.3",
    "@release-it/conventional-changelog": "^5.1.1",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.9",
    "@types/browser-update": "^3.3.0",
    "@types/jsdom": "^20.0.1",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7",
    "@types/shelljs": "^0.8.11",
    "@types/statuses": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "@vitejs/plugin-legacy": "^3.0.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vitest/coverage-c8": "^0.28.3",
    "@vitest/ui": "^0.28.3",
    "@vue-macros/volar": "^0.8.0",
    "@vue/test-utils": "^2.2.8",
    "@vue/tsconfig": "^0.1.3",
    "c8": "^7.12.0",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.33.0",
    "happy-dom": "^8.2.2",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jsdom": "^21.1.0",
    "lint-staged": "^13.1.0",
    "markdownlint-cli": "^0.33.0",
    "npm-check-updates": "^16.6.3",
    "npm-run-all": "^4.1.5",
    "picocolors": "^1.0.0",
    "postcss": "^8.4.21",
    "postcss-preset-env": "^8.0.1",
    "prettier": "^2.8.3",
    "release-it": "^15.6.0",
    "rimraf": "^4.1.2",
    "sass": "^1.58.0",
    "shelljs": "^0.8.5",
    "stylelint": "^14.16.1",
    "tailwind-extensions": "^0.21.1",
    "tailwindcss": "^3.2.4",
    "terser": "^5.16.2",
    "tsx": "^3.12.2",
    "type-fest": "^3.5.4",
    "typescript": "^4.9.5",
    "unocss": "^0.49.2",
    "unplugin-auto-import": "^0.13.0",
    "unplugin-icons": "^0.15.2",
    "unplugin-vue-components": "^0.23.0",
    "unplugin-vue-macros": "^1.7.2",
    "unplugin-vue-router": "^0.3.2",
    "vite": "^4.0.4",
    "vite-plugin-compression2": "^0.6.2",
    "vite-plugin-eslint2": "^3.2.1",
    "vite-plugin-inspect": "^0.7.15",
    "vite-plugin-pages": "^0.28.0",
    "vite-plugin-stylelint": "^4.1.8",
    "vite-plugin-vue-layouts": "^0.7.0",
    "vitest": "^0.28.3",
    "vue-tsc": "^1.0.24"
  },
  "engines": {
    "node": ">= 14.18"
  }
}
