    "vite-plugin-inspect": "^0.7.38",
    "vite-plugin-pages": "^0.31.0",
    "vite-plugin-stylelint": "^5.1.1",
    "vite-plugin-vue-devtools": "^0.5.1",
    "vite-plugin-vue-layouts": "^0.8.0",
    "vite-plugin-warmup": "^0.1.0",
    "vitest": "^0.34.4",
    "vue-tsc": "^1.8.11"
  },
  "packageManager": "pnpm@8.7.5",
  "engines": {
    "node": "18"
  }
}
