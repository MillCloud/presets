{
  "name": "vue",
  "version": "0.0.0",
  "private": true,
  "description": "A boilerplate for vue3.",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "check:deps": "ncu",
    "check:types": "vue-tsc --noEmit",
    "commit": "cz",
    "dev": "vite",
    "lint": "run-p lint:eslint lint:markdownlint lint:stylelint check:types",
    "lint:eslint": "eslint . --fix --ext=.js,.cjs,.mjs,.jsx,.ts,.cts,.mts,.tsx,.vue --ignore-path=.gitignore --cache --cache-location=node_modules/.vite/vite-plugin-eslint",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "lint:stylelint": "stylelint \"./**/*.{css,less,scss,sass,vue}\" --fix --allow-empty-input --ignore-path=.gitignore --cache --cache-location=node_modules/.vite/vite-plugin-stylelint",
    "prepare": "is-ci || husky install",
    "release": "run-s release-it build",
    "release-it": "release-it",
    "preview": "vite preview",
    "test:unit": "vitest"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.9",
    "@formily/core": "^2.2.2",
    "@formily/element-plus": "^1.0.0-beta.6",
    "@formily/vue": "^2.2.2",
    "@iconify/vue": "^3.2.1",
    "@vueuse/core": "^9.1.1",
    "@vueuse/router": "^9.1.1",
    "axios": "^0.27.2",
    "browser-update": "^3.3.40",
    "change-case": "^4.1.2",
    "color": "^4.2.3",
    "dayjs": "^1.11.5",
    "element-plus": "^2.2.15",
    "element-pro-components": "^1.2.1",
    "lodash-es": "^4.17.21",
    "modern-normalize": "^1.1.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.21",
    "qs": "^6.11.0",
    "ramda": "^0.28.0",
    "statuses": "^2.0.1",
    "vue": "^3.2.38",
    "vue-query": "^1.26.0",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/eslint-parser": "^7.18.9",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@commitlint/prompt": "^17.1.2",
    "@iconify/json": "^2.1.101",
    "@modyqyw/fabric": "^5.9.0",
    "@modyqyw/tailwind-presets": "^0.16.0",
    "@modyqyw/vite-plugin-eslint": "^2.0.9",
    "@release-it/conventional-changelog": "^5.1.0",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.4",
    "@types/browser-update": "^3.3.0",
    "@types/color": "^3.0.3",
    "@types/jsdom": "^20.0.0",
    "@types/lodash-es": "^4.17.6",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7",
    "@types/ramda": "^0.28.15",
    "@types/shelljs": "^0.8.11",
    "@types/statuses": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "@vitest/coverage-c8": "^0.22.1",
    "@vitest/ui": "^0.23.0",
    "@vue/test-utils": "^2.0.2",
    "@vue/tsconfig": "^0.1.3",
    "c8": "^7.12.0",
    "commitizen": "^4.2.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.0",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "jsdom": "^20.0.0",
    "lint-staged": "^13.0.3",
    "markdownlint-cli": "^0.32.2",
    "npm-check-updates": "^16.0.6",
    "npm-run-all": "^4.1.5",
    "picocolors": "^1.0.0",
    "postcss": "^8.4.16",
    "postcss-preset-env": "^7.8.0",
    "prettier": "^2.7.1",
    "release-it": "^15.4.1",
    "rimraf": "^3.0.2",
    "sass": "^1.54.8",
    "shelljs": "^0.8.5",
    "stylelint": "^14.11.0",
    "tailwindcss": "^3.1.8",
    "tsx": "^3.9.0",
    "type-fest": "^2.19.0",
    "typescript": "^4.8.2",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-icons": "^0.14.8",
    "unplugin-vue-components": "^0.22.4",
    "unplugin-vue-macros": "^0.11.1",
    "vite": "^3.0.9",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-inspect": "^0.6.1",
    "vite-plugin-mkcert": "^1.9.0",
    "vite-plugin-pages": "^0.25.0",
    "vite-plugin-stylelint": "^3.0.7",
    "vite-plugin-vue-layouts": "^0.7.0",
    "vitest": "^0.22.1",
    "vue-tsc": "^0.40.5"
  }
}
