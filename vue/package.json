{
  "name": "vue",
  "version": "0.0.0",
  "private": true,
  "description": "A boilerplate for vue3.",
  "type": "module",
  "scripts": {
    "build": "cross-env VITE_MODE=production vite build",
    "build:dev": "cross-env VITE_MODE=development vite build",
    "build:prod": "pnpm run build",
    "build:staging": "cross-env VITE_MODE=staging vite build",
    "check:deps": "taze",
    "check:types": "vue-tsc --noEmit",
    "commit": "git-cz",
    "dev": "cross-env VITE_MODE=development vite",
    "dev:dev": "pnpm run dev",
    "dev:prod": "cross-env VITE_MODE=production vite",
    "dev:staging": "cross-env VITE_MODE=staging vite",
    "lint": "conc \"pnpm:lint:eslint\" \"pnpm:lint:markdownlint\" \"pnpm:lint:stylelint\"",
    "lint:eslint": "eslint . --fix --cache --ignore-path=.gitignore",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "lint:stylelint": "stylelint \"./**/*.{css,scss,vue}\" --fix --cache --ignore-path=.gitignore",
    "prepare": "is-ci || simple-git-hooks",
    "preview": "vite preview",
    "release": "commit-and-tag-version -a",
    "test": "vitest run --coverage --passWithNoTests"
  },
  "dependencies": {
    "@ctrl/tinycolor": "^3.6.0",
    "@element-plus/icons-vue": "^2.1.0",
    "@iconify/vue": "^4.1.1",
    "@modyqyw/utils": "^6.0.0",
    "@tanstack/vue-query": "^4.29.5",
    "@vueuse/core": "^10.1.2",
    "@vueuse/router": "^10.1.2",
    "axios": "^1.4.0",
    "browser-update": "^3.3.44",
    "change-case": "^4.1.2",
    "dayjs": "^1.11.7",
    "element-plus": "^2.3.4",
    "modern-normalize": "^1.1.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.36",
    "qs": "^6.11.1",
    "statuses": "^2.0.1",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@commitlint/prompt": "^17.6.3",
    "@iconify/json": "^2.2.63",
    "@modyqyw/fabric": "^8.0.3",
    "@types/browser-update": "^3.3.0",
    "@types/eslint": "^8.37.0",
    "@types/node": "^18.16.6",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7",
    "@types/shelljs": "^0.8.12",
    "@types/statuses": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "@unocss/reset": "^0.51.12",
    "@vitejs/plugin-legacy": "^4.0.3",
    "@vitejs/plugin-vue": "^4.2.1",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vitest/coverage-c8": "^0.31.0",
    "@vitest/ui": "^0.31.0",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.3.2",
    "c8": "^7.13.0",
    "commitizen": "^4.3.0",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.40.0",
    "happy-dom": "^9.10.9",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.2.2",
    "markdownlint-cli": "^0.34.0",
    "picocolors": "^1.0.0",
    "postcss": "^8.4.23",
    "postcss-preset-env": "^8.3.2",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "sass": "^1.62.1",
    "shelljs": "^0.8.5",
    "simple-git": "^3.18.0",
    "simple-git-hooks": "^2.8.1",
    "sort-package-json": "^2.4.1",
    "stylelint": "^15.6.1",
    "taze": "^0.10.1",
    "terser": "^5.17.3",
    "tsx": "^3.12.7",
    "type-fest": "^3.10.0",
    "typescript": "^5.0.4",
    "unocss": "^0.51.12",
    "unocss-preset-element-plus": "^0.0.3",
    "unplugin-auto-import": "^0.15.3",
    "unplugin-icons": "^0.16.1",
    "unplugin-vue-components": "^0.24.1",
    "unplugin-vue-define-options": "^1.3.5",
    "vite": "^4.3.5",
    "vite-plugin-compression2": "^0.9.1",
    "vite-plugin-inspect": "^0.7.26",
    "vite-plugin-pages": "^0.29.0",
    "vite-plugin-vue-layouts": "^0.8.0",
    "vite-plugin-warmup": "^0.1.0",
    "vitest": "^0.31.0",
    "vue-tsc": "^1.6.4"
  },
  "packageManager": "pnpm@8.5.0",
  "engines": {
    "node": "18"
  }
}
