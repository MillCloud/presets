{
  "name": "vue",
  "version": "0.0.0",
  "private": true,
  "description": "A boilerplate for vue3.",
  "scripts": {
    "build": "vue-tsc --noEmit && vite build",
    "check:deps": "ncu",
    "check:types": "vue-tsc --noEmit",
    "commit": "git-cz",
    "dev": "vite",
    "lint": "pnpm run lint:eslint && pnpm run lint:markdownlint && pnpm run lint:stylelint && vue-tsc --noEmit",
    "lint:eslint": "eslint . --fix --ext=.js,.cjs,.mjs,.jsx,.ts,.cts,.mts,.tsx,.vue --ignore-path=.gitignore --cache --cache-location=node_modules/.vite/vite-plugin-eslint",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "lint:stylelint": "stylelint \"./**/*.{css,less,scss,sass,vue}\" --fix --allow-empty-input --ignore-path=.gitignore --cache --cache-location=node_modules/.vite/vite-plugin-stylelint",
    "prepare": "is-ci || husky install",
    "release": "release-it && pnpm run build",
    "release-it": "release-it",
    "serve": "vite preview"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/prompt"
    }
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.6",
    "@iconify/vue": "^3.2.1",
    "@vueuse/core": "^8.9.4",
    "@vueuse/router": "^8.9.4",
    "axios": "^0.27.2",
    "browser-update": "^3.3.40",
    "change-case": "^4.1.2",
    "color": "^4.2.3",
    "dayjs": "^1.11.4",
    "element-plus": "^2.2.9",
    "lodash-es": "^4.17.21",
    "modern-normalize": "^1.1.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.16",
    "qs": "^6.11.0",
    "ramda": "^0.28.0",
    "statuses": "^2.0.1",
    "vue": "^3.2.37",
    "vue-query": "^1.25.2",
    "vue-router": "^4.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/eslint-parser": "^7.18.9",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@commitlint/prompt": "^17.0.3",
    "@iconify/json": "^2.1.81",
    "@modyqyw/fabric": "^5.5.4",
    "@modyqyw/tailwind-presets": "^0.11.2",
    "@modyqyw/vite-plugin-eslint": "^2.0.6",
    "@release-it/conventional-changelog": "^5.0.0",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.4",
    "@types/browser-update": "^3.3.0",
    "@types/color": "^3.0.3",
    "@types/lodash-es": "^4.17.6",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7",
    "@types/ramda": "^0.28.15",
    "@types/statuses": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "@vue/tsconfig": "^0.1.3",
    "commitizen": "^4.2.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.20.0",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.0.3",
    "markdownlint-cli": "^0.32.0",
    "npm-check-updates": "^15.3.4",
    "picocolors": "^1.0.0",
    "postcss": "^8.4.14",
    "postcss-preset-env": "^7.7.2",
    "prettier": "^2.7.1",
    "release-it": "^15.1.3",
    "rimraf": "^3.0.2",
    "sass": "^1.53.0",
    "shelljs": "^0.8.5",
    "stylelint": "^14.9.1",
    "tailwindcss": "^3.1.6",
    "type-fest": "^2.17.0",
    "typescript": "^4.7.4",
    "unplugin-auto-import": "^0.10.0",
    "unplugin-icons": "^0.14.7",
    "unplugin-vue-components": "^0.21.1",
    "unplugin-vue-define-options": "^0.6.2",
    "vite": "^3.0.2",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-env-compatible": "^1.1.1",
    "vite-plugin-inspect": "^0.6.0",
    "vite-plugin-mkcert": "^1.9.0",
    "vite-plugin-pages": "^0.25.0",
    "vite-plugin-stylelint": "^3.0.4",
    "vite-plugin-vue-layouts": "^0.7.0",
    "vue-tsc": "^0.38.9"
  }
}
