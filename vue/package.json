    "@vitest/ui": "^0.34.6",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "commitizen": "^4.3.0",
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.3",
