{
  "name": "vue",
  "version": "0.0.0",
  "private": true,
  "description": "A boilerplate for vue3.",
  "type": "module",
  "scripts": {
    "build": "cross-env VITE_MODE=production vite build",
    "build:dev": "cross-env VITE_MODE=development vite build",
    "build:prod": "pnpm run build",
    "build:staging": "cross-env VITE_MODE=staging vite build",
    "check:deps": "taze -f",
    "check:types": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "commit": "git-cz",
    "dev": "cross-env VITE_MODE=development vite",
    "dev:dev": "pnpm run dev",
    "dev:prod": "cross-env VITE_MODE=production vite",
    "dev:staging": "cross-env VITE_MODE=staging vite",
    "lint": "conc \"pnpm:lint:eslint\" \"pnpm:lint:markdownlint\" \"pnpm:lint:stylelint\"",
    "lint:eslint": "eslint . --fix --cache --ignore-path=.gitignore",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "lint:stylelint": "stylelint \"./**/*.{css,scss,vue}\" --fix --cache --ignore-path=.gitignore",
    "prepare": "is-ci || simple-git-hooks",
    "preview": "vite preview",
    "release": "pnpm install && commit-and-tag-version -a",
    "test:unit": "vitest run --coverage"
  },
  "dependencies": {
    "@ctrl/tinycolor": "^4.0.2",
    "@iconify/vue": "^4.1.1",
    "@modyqyw/utils": "^8.0.0",
    "@tanstack/vue-query": "^4.35.3",
    "@vueuse/core": "^10.4.1",
    "@vueuse/router": "^10.4.1",
    "axios": "^1.5.0",
    "browser-update": "^3.3.44",
    "change-case": "^4.1.2",
    "dayjs": "^1.11.9",
    "modern-normalize": "^2.0.0",
    "naive-ui": "^2.34.4",
    "naive-ui-pro-components": "0.3.0-beta.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.6",
    "qs": "^6.11.2",
    "statuses": "^2.0.1",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@commitlint/prompt": "^17.7.1",
    "@iconify/json": "^2.2.115",
    "@modyqyw/fabric": "^9.0.5",
    "@plugin-web-update-notification/vite": "^1.6.5",
    "@sicons/antd": "^0.12.0",
    "@sicons/carbon": "^0.12.0",
    "@sicons/fa": "^0.12.0",
    "@sicons/fluent": "^0.12.0",
    "@sicons/ionicons4": "^0.12.0",
    "@sicons/ionicons5": "^0.12.0",
    "@sicons/material": "^0.12.0",
    "@sicons/tabler": "^0.12.0",
    "@tsconfig/node18": "^18.2.2",
    "@types/browser-update": "^3.3.0",
    "@types/eslint": "^8.44.2",
    "@types/jsdom": "^21.1.2",
    "@types/node": "^18.17.15",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.8",
    "@types/shelljs": "^0.8.12",
    "@types/statuses": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@unocss/reset": "^0.55.7",
    "@vicons/antd": "^0.12.0",
    "@vicons/carbon": "^0.12.0",
    "@vicons/fa": "^0.12.0",
    "@vicons/fluent": "^0.12.0",
    "@vicons/ionicons4": "^0.12.0",
