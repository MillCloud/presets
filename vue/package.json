{
  "name": "vue",
  "version": "0.0.0",
  "private": true,
  "description": "A boilerplate for vue3.",
  "type": "module",
  "scripts": {
    "build": "cross-env VITE_MODE=production vite build",
    "build:dev": "cross-env VITE_MODE=development vite build",
    "build:prod": "pnpm run build",
    "build:staging": "cross-env VITE_MODE=staging vite build",
    "check:deps": "taze -f",
    "check:types": "vue-tsc --noEmit",
    "commit": "git-cz",
    "dev": "cross-env VITE_MODE=development vite",
    "dev:dev": "pnpm run dev",
    "dev:prod": "cross-env VITE_MODE=production vite",
    "dev:staging": "cross-env VITE_MODE=staging vite",
    "lint": "conc \"pnpm:lint:eslint\" \"pnpm:lint:markdownlint\" \"pnpm:lint:stylelint\"",
    "lint:eslint": "eslint . --fix --cache --ignore-path=.gitignore",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "lint:stylelint": "stylelint \"./**/*.{css,scss,vue}\" --fix --cache --ignore-path=.gitignore",
    "prepare": "is-ci || simple-git-hooks",
    "preview": "vite preview",
    "release": "commit-and-tag-version -a",
    "test": "vitest run --coverage --passWithNoTests"
  },
  "dependencies": {
    "@ctrl/tinycolor": "^3.6.0",
    "@iconify/vue": "^4.1.1",
    "@modyqyw/utils": "^7.0.1",
    "@tanstack/vue-query": "^4.29.15",
    "@vueuse/core": "^10.2.0",
    "@vueuse/router": "^10.2.0",
    "axios": "^1.4.0",
    "browser-update": "^3.3.44",
    "change-case": "^4.1.2",
    "dayjs": "^1.11.8",
    "modern-normalize": "^2.0.0",
    "naive-ui": "^2.34.4",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.4",
    "qs": "^6.11.2",
    "statuses": "^2.0.1",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@commitlint/prompt": "^17.6.5",
    "@iconify/json": "^2.2.81",
    "@modyqyw/fabric": "^8.3.1",
    "@types/browser-update": "^3.3.0",
    "@types/eslint": "^8.40.2",
    "@types/node": "^18.16.18",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7",
    "@types/shelljs": "^0.8.12",
    "@types/statuses": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "@unocss/reset": "^0.55.0",
    "@vitejs/plugin-legacy": "^4.0.5",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vitest/coverage-v8": "^0.32.2",
    "@vitest/ui": "^0.32.2",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.4.0",
    "commitizen": "^4.3.0",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.43.0",
    "happy-dom": "^9.20.3",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.2.2",
    "markdownlint-cli": "^0.35.0",
    "picocolors": "^1.0.0",
    "postcss": "^8.4.24",
    "postcss-preset-env": "^8.5.1",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "sass": "^1.63.5",
    "shelljs": "^0.8.5",
    "simple-git": "^3.19.0",
    "simple-git-hooks": "^2.8.1",
    "sort-package-json": "^2.4.1",
    "stylelint": "^15.8.0",
    "taze": "^0.10.2",
    "terser": "^5.18.1",
    "tsx": "^3.12.7",
    "type-fest": "^3.12.0",
    "typescript": "~5.0.4",
    "unocss": "^0.53.1",
    "unplugin-auto-import": "^0.16.4",
    "unplugin-icons": "^0.16.3",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.3.9",
    "vite-plugin-checker": "^0.6.1",
    "vite-plugin-compression2": "^0.9.2",
    "vite-plugin-inspect": "^0.7.28",
    "vite-plugin-pages": "^0.31.0",
    "vite-plugin-vue-devtools": "^0.2.1",
    "vite-plugin-vue-layouts": "^0.8.0",
    "vite-plugin-warmup": "^0.1.0",
    "vitest": "^0.32.2",
    "vue-tsc": "^1.8.1"
  },
  "packageManager": "pnpm@8.6.3",
  "engines": {
    "node": "18"
  }
}
