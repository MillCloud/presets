{
  "name": "nuxt",
  "version": "0.0.0",
  "private": true,
  "description": "A boilerplate for nuxt3.",
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "changelog": "conventional-changelog -p conventionalcommits -i CHANGELOG.md -s -u -r 0 && git add CHANGELOG.md",
    "check:deps": "ncu --dep dev,optional,prod,packageManager",
    "check:types": "nuxi typecheck",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "commit": "git-cz",
    "lint": "run-p lint:eslint lint:markdownlint lint:stylelint check:types",
    "lint:eslint": "eslint . --fix --cache --ignore-path=.gitignore",
    "lint:markdownlint": "markdownlint . --fix --ignore=CHANGELOG.md --ignore-path=.gitignore",
    "lint:stylelint": "stylelint \"./**/*.{css,scss,vue}\" --fix --cache --ignore-path=.gitignore",
    "prepare": "is-ci || husky install",
    "release": "pnpm install && pnpm run lint && bumpp --all -x \"pnpm run changelog\"",
    "test:unit": "vitest run --coverage"
  },
  "dependencies": {
    "@ctrl/tinycolor": "^3.6.0",
    "@element-plus/icons-vue": "^2.1.0",
    "@iconify/vue": "^4.1.0",
    "@modyqyw/utils": "^5.0.0",
    "@pinia/nuxt": "^0.4.7",
    "@tanstack/vue-query": "^4.27.0",
    "@vueuse/core": "^9.13.0",
    "axios": "^1.3.4",
    "browser-update": "^3.3.44",
    "change-case": "^4.1.2",
    "dayjs": "^1.11.7",
    "element-plus": "^2.3.1",
    "modern-normalize": "^1.1.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.33",
    "qs": "^6.11.1",
    "statuses": "^2.0.1",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@commitlint/prompt": "^17.4.4",
    "@egoist/tailwindcss-icons": "^1.0.7",
    "@iconify/json": "^2.2.38",
    "@modyqyw/fabric": "^7.4.1",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.9",
    "@types/browser-update": "^3.3.0",
    "@types/eslint": "^8.21.3",
    "@types/jsdom": "^21.1.0",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7",
    "@types/shelljs": "^0.8.11",
    "@types/statuses": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vitejs/plugin-legacy": "^4.0.2",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vitest/coverage-c8": "^0.29.7",
    "@vitest/ui": "^0.29.7",
    "@vueuse/nuxt": "^9.13.0",
    "bumpp": "^9.0.0",
    "c8": "^7.13.0",
    "commitizen": "^4.3.0",
    "conventional-changelog-cli": "^2.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.36.0",
    "happy-dom": "^8.9.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jsdom": "^21.1.1",
    "lint-staged": "^13.2.0",
    "markdownlint-cli": "^0.33.0",
    "npm-check-updates": "^16.7.13",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.3.1",
    "nuxt-icon": "^0.3.3",
    "nuxt-typed-router": "^3.0.6",
    "nuxt-vitest": "^0.6.9",
    "picocolors": "^1.0.0",
    "postcss": "^8.4.21",
    "postcss-preset-env": "^8.0.1",
    "prettier": "^2.8.6",
    "rimraf": "^4.4.0",
    "sass": "^1.59.3",
    "shelljs": "^0.8.5",
    "sort-package-json": "^2.4.1",
    "stylelint": "^15.3.0",
    "tailwind-extensions": "^0.22.1",
    "tailwindcss": "^3.2.7",
    "terser": "^5.16.6",
    "tsx": "^3.12.6",
    "type-fest": "^3.6.1",
    "typescript": "^4.9.5",
    "unplugin-vue-define-options": "^1.3.0",
    "vite": "^4.2.1",
    "vitest": "^0.29.7",
    "vue-tsc": "^1.2.0"
  },
  "engines": {
    "node": ">= 14.18"
  }
}
