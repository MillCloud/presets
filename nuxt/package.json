{
  "name": "nuxt",
  "version": "0.0.0",
  "private": true,
  "description": "A boilerplate for nuxt3.",
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "check:deps": "ncu --dep dev,optional,prod,packageManager",
    "check:types": "nuxi typecheck",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "commit": "git-cz",
    "lint": "run-p lint:eslint lint:markdownlint lint:stylelint check:types",
    "lint:eslint": "eslint . --fix --cache --no-error-on-unmatched-pattern",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "lint:stylelint": "stylelint \"./**/*.{css,scss,vue}\" --fix --cache --allow-empty-input --ignore-path=.gitignore",
    "prepare": "is-ci || husky install",
    "release": "run-s release-it build",
    "release-it": "release-it",
    "test:unit": "vitest run --coverage"
  },
  "dependencies": {
    "@ctrl/tinycolor": "^3.5.0",
    "@element-plus/icons-vue": "^2.0.10",
    "@iconify/vue": "^4.1.0",
    "@modyqyw/utils": "^4.1.0",
    "@pinia/nuxt": "^0.4.6",
    "@tanstack/vue-query": "^4.24.6",
    "@vueuse/core": "^9.12.0",
    "axios": "^1.3.2",
    "browser-update": "^3.3.44",
    "change-case": "^4.1.2",
    "dayjs": "^1.11.7",
    "element-plus": "^2.2.30",
    "modern-normalize": "^1.1.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.30",
    "qs": "^6.11.0",
    "statuses": "^2.0.1",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/eslint-parser": "^7.19.1",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@commitlint/prompt": "^17.4.2",
    "@iconify/json": "^2.2.20",
    "@modyqyw/fabric": "^7.0.1",
    "@nuxt/devtools": "^0.1.2",
    "@nuxt/test-utils-edge": "3.2.1-rc.0-27934134.7111b6c",
    "@nuxtjs/tailwindcss": "^6.4.0",
    "@release-it/conventional-changelog": "^5.1.1",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.9",
    "@types/browser-update": "^3.3.0",
    "@types/eslint": "^8.21.0",
    "@types/jsdom": "^21.1.0",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7",
    "@types/shelljs": "^0.8.11",
    "@types/statuses": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@unocss/nuxt": "^0.49.4",
    "@vitejs/plugin-legacy": "^4.0.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vitest/coverage-c8": "^0.28.4",
    "@vitest/ui": "^0.28.4",
    "@vueuse/nuxt": "^9.12.0",
    "c8": "^7.12.0",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.34.0",
    "happy-dom": "^8.2.6",
    "husky": "^8.0.3",
