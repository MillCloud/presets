    "picocolors": "^1.0.0",
    "postcss": "^8.4.21",
    "postcss-preset-env": "^8.0.1",
    "prettier": "^2.8.4",
    "release-it": "^15.6.0",
    "rimraf": "^4.1.2",
    "sass": "^1.58.0",
    "shelljs": "^0.8.5",
    "stylelint": "^15.1.0",
    "tailwind-extensions": "^0.21.2",
    "tailwindcss": "^3.2.6",
    "terser": "^5.16.3",
    "tsx": "^3.12.3",
    "type-fest": "^3.5.7",
    "typescript": "^4.9.5",
    "unocss": "^0.49.4",
    "unplugin-icons": "^0.15.2",
    "unplugin-vue-define-options": "^1.2.1",
