    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "@vue/tsconfig": "^0.1.3",
    "commitizen": "^4.2.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.20.0",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.0.3",
    "markdownlint-cli": "^0.32.0",
    "npm-check-updates": "^15.3.4",
    "picocolors": "^1.0.0",
    "postcss": "^8.4.14",
