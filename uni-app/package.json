{
  "name": "uni-app",
  "version": "0.0.0",
  "private": true,
  "description": "A boilerplate for uni-app (vue3).",
  "scripts": {
    "build:app": "uni build -p app",
    "build:custom": "uni build -p",
    "build:h5": "uni build",
    "build:h5:ssr": "uni build --ssr",
    "build:mp-alipay": "run-s build:mp-alipay-no-patch patch:mp-alipay",
    "build:mp-alipay-no-patch": "cross-env UNI_OUTPUT_DIR=dist/mp-alipay uni build -p mp-alipay",
    "build:mp-baidu": "cross-env UNI_OUTPUT_DIR=dist/mp-baidu uni build -p mp-baidu",
    "build:mp-kuaishou": "cross-env UNI_OUTPUT_DIR=dist/mp-kuaishou uni build -p mp-kuaishou",
    "build:mp-lark": "cross-env UNI_OUTPUT_DIR=dist/mp-lark uni build -p mp-lark",
    "build:mp-qq": "cross-env UNI_OUTPUT_DIR=dist/mp-qq uni build -p mp-qq",
    "build:mp-toutiao": "cross-env UNI_OUTPUT_DIR=dist/mp-toutiao uni build -p mp-toutiao",
    "build:mp-weixin": "cross-env UNI_OUTPUT_DIR=dist/mp-weixin uni build -p mp-weixin",
    "build:quickapp-webview": "cross-env UNI_OUTPUT_DIR=dist/quickapp-webview uni build -p quickapp-webview",
    "build:quickapp-webview-huawei": "cross-env UNI_OUTPUT_DIR=dist/quickapp-webview-huawei uni build -p quickapp-webview-huawei",
    "build:quickapp-webview-union": "cross-env UNI_OUTPUT_DIR=dist/quickapp-webview-union uni build -p quickapp-webview-union",
    "check:deps": "ncu --dep dev,optional,prod,packageManager",
    "check:types": "vue-tsc --noEmit",
    "commit": "git-cz",
    "dev:app": "uni -p app",
    "dev:custom": "uni -p",
    "dev:h5": "uni",
    "dev:h5:ssr": "uni --ssr",
    "dev:mp-alipay": "run-p dev:mp-alipay-no-patch patch:mp-alipay:watch",
    "dev:mp-alipay-no-patch": "cross-env UNI_OUTPUT_DIR=dist/mp-alipay uni -p mp-alipay",
    "dev:mp-baidu": "cross-env UNI_OUTPUT_DIR=dist/mp-baidu uni -p mp-baidu",
    "dev:mp-kuaishou": "cross-env UNI_OUTPUT_DIR=dist/mp-kuaishou uni -p mp-kuaishou",
    "dev:mp-lark": "cross-env UNI_OUTPUT_DIR=dist/mp-lark uni -p mp-lark",
    "dev:mp-qq": "cross-env UNI_OUTPUT_DIR=dist/mp-qq uni -p mp-qq",
    "dev:mp-toutiao": "cross-env UNI_OUTPUT_DIR=dist/mp-toutiao uni -p mp-toutiao",
    "dev:mp-weixin": "cross-env UNI_OUTPUT_DIR=dist/mp-weixin uni -p mp-weixin",
    "dev:quickapp-webview": "cross-env UNI_OUTPUT_DIR=dist/quickapp-webview uni -p quickapp-webview",
    "dev:quickapp-webview-huawei": "cross-env UNI_OUTPUT_DIR=dist/quickapp-webview-huawei uni -p quickapp-webview-huawei",
    "dev:quickapp-webview-union": "cross-env UNI_OUTPUT_DIR=dist/quickapp-webview-union uni -p quickapp-webview-union",
    "lint": "run-p lint:eslint lint:markdownlint lint:stylelint check:types",
    "lint:eslint": "eslint . --ext=.js,.cjs,.mjs,.jsx,.ts,.cts,.mts,.tsx,.vue  --fix --cache --no-error-on-unmatched-pattern --ignore-path=.gitignore",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "lint:stylelint": "stylelint \"./**/*.{css,less,scss,sass,vue}\" --fix --cache --allow-empty-input --ignore-path=.gitignore",
    "prepare": "is-ci || husky install",
    "release": "release-it",
    "patch:mp-alipay": "tsx ./scripts/patch-mp-alipay.ts",
    "patch:mp-alipay:watch": "nodemon --watch dist/mp-alipay/pages --ext axml --delay 0.5 --exec \"tsx\" ./scripts/patch-mp-alipay.ts",
    "test:unit": "vitest run --coverage"
  },
  "dependencies": {
    "@ctrl/tinycolor": "^3.5.0",
    "@dcloudio/uni-app": "3.0.0-alpha-3061620221230002",
    "@dcloudio/uni-app-plus": "3.0.0-alpha-3061620221230002",
    "@dcloudio/uni-components": "3.0.0-alpha-3061620221230002",
    "@dcloudio/uni-h5": "3.0.0-alpha-3061620221230002",
    "@dcloudio/uni-mp-alipay": "3.0.0-alpha-3061620221230002",
    "@dcloudio/uni-mp-baidu": "3.0.0-alpha-3061620221230002",
    "@dcloudio/uni-mp-kuaishou": "3.0.0-alpha-3061620221230002",
    "@dcloudio/uni-mp-lark": "3.0.0-alpha-3061620221230002",
    "@dcloudio/uni-mp-qq": "3.0.0-alpha-3061620221230002",
    "@dcloudio/uni-mp-toutiao": "3.0.0-alpha-3061620221230002",
    "@dcloudio/uni-mp-weixin": "3.0.0-alpha-3061620221230002",
    "@dcloudio/uni-quickapp-webview": "3.0.0-alpha-3061620221230002",
    "@dcloudio/uni-ui": "^1.4.24",
    "@lukemorales/query-key-factory": "^1.0.3",
    "@tanstack/vue-query": "^4.22.0",
    "@uni-helper/uni-network": "^0.12.6",
    "@uni-helper/uni-promises": "^0.1.4",
    "@uni-helper/uni-use": "^0.11.0",
    "@vueuse/core": "^9.10.0",
    "change-case": "^4.1.2",
    "core-js": "^3.27.1",
    "dayjs": "^1.11.7",
    "lodash-es": "^4.17.21",
    "modern-normalize": "^1.1.0",
    "pinia": "^2.0.28",
    "qs": "^6.11.0",
    "ramda": "^0.28.0",
    "statuses": "^2.0.1",
    "vue": "^3.2.45",
    "vue-i18n": "^9.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/eslint-parser": "^7.19.1",
    "@commitlint/cli": "^17.4.1",
    "@commitlint/config-conventional": "^17.4.0",
    "@commitlint/prompt": "^17.4.1",
    "@dcloudio/types": "^3.2.7",
    "@dcloudio/uni-automator": "3.0.0-alpha-3061720230111001",
    "@dcloudio/uni-cli-shared": "3.0.0-alpha-3061620221230002",
    "@dcloudio/uni-stacktracey": "3.0.0-alpha-3061620221230002",
    "@dcloudio/vite-plugin-uni": "3.0.0-alpha-3061620221230002",
    "@iconify/json": "^2.2.5",
    "@mini-types/alipay": "^3.0.0",
    "@modyqyw/fabric": "^5.16.0",
    "@modyqyw/vite-plugin-eslint": "^3.1.6",
    "@release-it/conventional-changelog": "^5.1.1",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.9",
    "@types/lodash-es": "^4.17.6",
    "@types/qs": "^6.9.7",
    "@types/ramda": "^0.28.20",
    "@types/shelljs": "^0.8.11",
    "@types/statuses": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "@uni-helper/uni-app-types": "^0.3.1",
    "@uni-helper/uni-cloud-types": "^0.3.0",
    "@uni-helper/uni-deploy": "^0.3.0",
    "@uni-helper/uni-ui-types": "^0.3.2",
    "@uni-helper/vite-plugin-uni-tailwind": "^0.9.1",
    "@vitejs/plugin-legacy": "^3.0.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
