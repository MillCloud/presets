{
  "name": "uni-app",
  "version": "0.0.0",
  "private": true,
  "description": "A boilerplate for uni-app (vue3).",
  "scripts": {
    "build:app": "uni build -p app",
    "build:custom": "uni build -p",
    "build:h5": "uni build",
    "build:h5:ssr": "uni build --ssr",
    "build:mp-alipay": "run-s build:mp-alipay-no-patch patch:mp-alipay",
    "build:mp-alipay-no-patch": "cross-env UNI_OUTPUT_DIR=dist/mp-alipay uni build -p mp-alipay",
    "build:mp-baidu": "cross-env UNI_OUTPUT_DIR=dist/mp-baidu uni build -p mp-baidu",
    "build:mp-kuaishou": "cross-env UNI_OUTPUT_DIR=dist/mp-kuaishou uni build -p mp-kuaishou",
    "build:mp-lark": "cross-env UNI_OUTPUT_DIR=dist/mp-lark uni build -p mp-lark",
    "build:mp-qq": "cross-env UNI_OUTPUT_DIR=dist/mp-qq uni build -p mp-qq",
    "build:mp-toutiao": "cross-env UNI_OUTPUT_DIR=dist/mp-toutiao uni build -p mp-toutiao",
    "build:mp-weixin": "cross-env UNI_OUTPUT_DIR=dist/mp-weixin uni build -p mp-weixin",
    "build:quickapp-webview": "cross-env UNI_OUTPUT_DIR=dist/quickapp-webview uni build -p quickapp-webview",
    "build:quickapp-webview-huawei": "cross-env UNI_OUTPUT_DIR=dist/quickapp-webview-huawei uni build -p quickapp-webview-huawei",
    "build:quickapp-webview-union": "cross-env UNI_OUTPUT_DIR=dist/quickapp-webview-union uni build -p quickapp-webview-union",
    "changelog": "conventional-changelog -p conventionalcommits -i CHANGELOG.md -s -u -r 0 && git add CHANGELOG.md",
    "check:deps": "taze",
    "check:types": "vue-tsc --noEmit",
    "commit": "git-cz",
    "dev:app": "uni -p app",
    "dev:custom": "uni -p",
    "dev:h5": "uni",
    "dev:h5:ssr": "uni --ssr",
    "dev:mp-alipay": "run-p dev:mp-alipay-no-patch patch:mp-alipay:watch",
    "dev:mp-alipay-no-patch": "cross-env UNI_OUTPUT_DIR=dist/mp-alipay uni -p mp-alipay",
    "dev:mp-baidu": "cross-env UNI_OUTPUT_DIR=dist/mp-baidu uni -p mp-baidu",
    "dev:mp-kuaishou": "cross-env UNI_OUTPUT_DIR=dist/mp-kuaishou uni -p mp-kuaishou",
    "dev:mp-lark": "cross-env UNI_OUTPUT_DIR=dist/mp-lark uni -p mp-lark",
    "dev:mp-qq": "cross-env UNI_OUTPUT_DIR=dist/mp-qq uni -p mp-qq",
    "dev:mp-toutiao": "cross-env UNI_OUTPUT_DIR=dist/mp-toutiao uni -p mp-toutiao",
    "dev:mp-weixin": "cross-env UNI_OUTPUT_DIR=dist/mp-weixin uni -p mp-weixin",
    "dev:quickapp-webview": "cross-env UNI_OUTPUT_DIR=dist/quickapp-webview uni -p quickapp-webview",
    "dev:quickapp-webview-huawei": "cross-env UNI_OUTPUT_DIR=dist/quickapp-webview-huawei uni -p quickapp-webview-huawei",
    "dev:quickapp-webview-union": "cross-env UNI_OUTPUT_DIR=dist/quickapp-webview-union uni -p quickapp-webview-union",
    "lint": "run-p lint:eslint lint:markdownlint lint:stylelint check:types",
    "lint:eslint": "eslint . --fix --cache --ignore-path=.gitignore",
    "lint:markdownlint": "markdownlint . --fix --ignore=CHANGELOG.md --ignore-path=.gitignore",
    "lint:stylelint": "stylelint \"./**/*.{css,scss,vue}\" --fix --cache --ignore-path=.gitignore",
    "manifest": "tsx ./scripts/update-manifest.ts && git add ./src/manifest.json",
    "prepare": "is-ci || husky install",
    "release": "npm install && npm run lint && bumpp -x \"pnpm run manifest && pnpm run changelog\"",
    "patch:mp-alipay": "tsx ./scripts/patch-mp-alipay.ts",
    "patch:mp-alipay:watch": "nodemon --watch dist/mp-alipay/pages --ext axml --delay 0.5 --exec \"tsx\" ./scripts/patch-mp-alipay.ts",
    "test:unit": "vitest run --coverage"
  },
  "dependencies": {
    "@ctrl/tinycolor": "^3.6.0",
    "@dcloudio/uni-app": "3.0.0-alpha-3070620230227001",
    "@dcloudio/uni-app-plus": "3.0.0-alpha-3070620230227001",
    "@dcloudio/uni-components": "3.0.0-alpha-3070620230227001",
    "@dcloudio/uni-h5": "3.0.0-alpha-3070620230227001",
    "@dcloudio/uni-mp-alipay": "3.0.0-alpha-3070620230227001",
    "@dcloudio/uni-mp-baidu": "3.0.0-alpha-3070620230227001",
    "@dcloudio/uni-mp-jd": "3.0.0-alpha-3070620230227001",
    "@dcloudio/uni-mp-kuaishou": "3.0.0-alpha-3070620230227001",
    "@dcloudio/uni-mp-lark": "3.0.0-alpha-3070620230227001",
    "@dcloudio/uni-mp-qq": "3.0.0-alpha-3070620230227001",
    "@dcloudio/uni-mp-toutiao": "3.0.0-alpha-3070620230227001",
    "@dcloudio/uni-mp-weixin": "3.0.0-alpha-3070620230227001",
    "@dcloudio/uni-quickapp-webview": "3.0.0-alpha-3070620230227001",
    "@dcloudio/uni-ui": "^1.4.26",
    "@modyqyw/utils": "^5.0.0",
    "@tanstack/vue-query": "^4.27.0",
    "@uni-helper/uni-network": "^0.14.0",
    "@uni-helper/uni-promises": "^0.1.5",
    "@uni-helper/uni-use": "^0.12.0",
    "@vueuse/core": "^9.13.0",
    "change-case": "^4.1.2",
    "core-js": "^3.29.1",
    "dayjs": "^1.11.7",
    "modern-normalize": "^1.1.0",
    "pinia": "^2.0.33",
    "qs": "^6.11.1",
    "statuses": "^2.0.1",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@commitlint/prompt": "^17.5.0",
    "@dcloudio/types": "^3.2.11",
    "@dcloudio/uni-automator": "3.0.0-alpha-3070620230227001",
    "@dcloudio/uni-cli-shared": "3.0.0-alpha-3070620230227001",
    "@dcloudio/uni-stacktracey": "3.0.0-alpha-3070620230227001",
    "@dcloudio/vite-plugin-uni": "3.0.0-alpha-3070620230227001",
    "@egoist/tailwindcss-icons": "^1.0.7",
    "@iconify/json": "^2.2.40",
    "@mini-types/alipay": "^3.0.7",
    "@modyqyw/fabric": "^7.5.0",
    "@types/eslint": "^8.21.3",
    "@types/qs": "^6.9.7",
    "@types/shelljs": "^0.8.11",
    "@types/statuses": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@uni-helper/uni-app-types": "^0.5.3",
    "@uni-helper/uni-cloud-types": "^0.5.0",
    "@uni-helper/uni-deploy": "^0.3.0",
    "@uni-helper/uni-ui-types": "^0.5.2",
    "@uni-helper/vite-plugin-uni-tailwind": "^0.9.1",
    "@vitejs/plugin-legacy": "^4.0.2",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vitest/coverage-c8": "^0.29.8",
    "@vitest/ui": "^0.29.8",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.1.3",
    "bumpp": "^9.0.0",
    "c8": "^7.13.0",
    "commitizen": "^4.3.0",
    "conventional-changelog-cli": "^2.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.37.0",
    "happy-dom": "^8.9.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.2.0",
    "markdownlint-cli": "^0.33.0",
    "miniprogram-api-typings": "^3.9.0",
    "npm-run-all": "^4.1.5",
    "picocolors": "^1.0.0",
    "pino": "^8.11.0",
    "pino-pretty": "^10.0.0",
    "postcss": "^8.4.21",
    "postcss-preset-env": "^8.2.0",
    "prettier": "^2.8.7",
    "rimraf": "^4.4.1",
    "sass": "^1.60.0",
    "shelljs": "^0.8.5",
    "sort-package-json": "^2.4.1",
    "stylelint": "^15.3.0",
    "tailwind-extensions": "^0.22.1",
    "tailwindcss": "^3.3.0",
    "taze": "^0.9.1",
    "terser": "^5.16.8",
    "tsx": "^3.12.6",
    "type-fest": "^3.7.1",
    "typescript": "^4.9.5",
    "unplugin-auto-import": "^0.15.2",
    "unplugin-vue-components": "^0.24.1",
    "unplugin-vue-define-options": "^1.3.2",
    "vite": "^4.2.1",
    "vitest": "^0.29.8",
    "vue-tsc": "^1.2.0"
  },
  "engines": {
    "node": ">= 14.18"
  }
}
