    "npm-check-updates": "^16.6.2",
    "npm-run-all": "^4.1.5",
    "picocolors": "^1.0.0",
    "pino": "^8.8.0",
    "pino-pretty": "^9.1.1",
    "postcss": "^8.4.21",
    "postcss-preset-env": "^7.8.3",
    "prettier": "^2.8.2",
    "release-it": "^15.6.0",
    "rimraf": "^3.0.2",
    "sass": "^1.57.1",
    "shelljs": "^0.8.5",
    "stylelint": "^14.16.1",
    "tailwind-extensions": "^0.21.1",
