{
  "name": "uni-app",
  "version": "0.0.0",
  "private": true,
  "description": "A boilerplate for uni-app (vue3).",
  "scripts": {
    "build:app": "uni build -p app",
    "build:custom": "uni build -p",
    "build:h5": "uni build",
    "build:h5:ssr": "uni build --ssr",
    "build:mp-alipay": "run-s build:mp-alipay-no-patch patch:mp-alipay",
    "build:mp-alipay-no-patch": "cross-env UNI_OUTPUT_DIR=dist/mp-alipay uni build -p mp-alipay",
    "build:mp-baidu": "cross-env UNI_OUTPUT_DIR=dist/mp-baidu uni build -p mp-baidu",
    "build:mp-kuaishou": "cross-env UNI_OUTPUT_DIR=dist/mp-kuaishou uni build -p mp-kuaishou",
    "build:mp-lark": "cross-env UNI_OUTPUT_DIR=dist/mp-lark uni build -p mp-lark",
    "build:mp-qq": "cross-env UNI_OUTPUT_DIR=dist/mp-qq uni build -p mp-qq",
    "build:mp-toutiao": "cross-env UNI_OUTPUT_DIR=dist/mp-toutiao uni build -p mp-toutiao",
    "build:mp-weixin": "cross-env UNI_OUTPUT_DIR=dist/mp-weixin uni build -p mp-weixin",
    "build:quickapp-webview": "cross-env UNI_OUTPUT_DIR=dist/quickapp-webview uni build -p quickapp-webview",
    "build:quickapp-webview-huawei": "cross-env UNI_OUTPUT_DIR=dist/quickapp-webview-huawei uni build -p quickapp-webview-huawei",
    "build:quickapp-webview-union": "cross-env UNI_OUTPUT_DIR=dist/quickapp-webview-union uni build -p quickapp-webview-union",
    "check:deps": "ncu",
    "check:types": "vue-tsc --noEmit",
    "commit": "cz",
    "dev:app": "uni -p app",
    "dev:custom": "uni -p",
    "dev:h5": "uni",
    "dev:h5:ssr": "uni --ssr",
    "dev:mp-alipay": "run-p dev:mp-alipay-no-patch patch:mp-alipay:watch",
    "dev:mp-alipay-no-patch": "cross-env UNI_OUTPUT_DIR=dist/mp-alipay uni -p mp-alipay",
    "dev:mp-baidu": "cross-env UNI_OUTPUT_DIR=dist/mp-baidu uni -p mp-baidu",
    "dev:mp-kuaishou": "cross-env UNI_OUTPUT_DIR=dist/mp-kuaishou uni -p mp-kuaishou",
    "dev:mp-lark": "cross-env UNI_OUTPUT_DIR=dist/mp-lark uni -p mp-lark",
    "dev:mp-qq": "cross-env UNI_OUTPUT_DIR=dist/mp-qq uni -p mp-qq",
    "dev:mp-toutiao": "cross-env UNI_OUTPUT_DIR=dist/mp-toutiao uni -p mp-toutiao",
    "dev:mp-weixin": "cross-env UNI_OUTPUT_DIR=dist/mp-weixin uni -p mp-weixin",
    "dev:quickapp-webview": "cross-env UNI_OUTPUT_DIR=dist/quickapp-webview uni -p quickapp-webview",
    "dev:quickapp-webview-huawei": "cross-env UNI_OUTPUT_DIR=dist/quickapp-webview-huawei uni -p quickapp-webview-huawei",
    "dev:quickapp-webview-union": "cross-env UNI_OUTPUT_DIR=dist/quickapp-webview-union uni -p quickapp-webview-union",
    "lint": "run-p lint:eslint lint:markdownlint lint:stylelint check:types",
    "lint:eslint": "eslint . --fix --ext=.js,.cjs,.mjs,.jsx,.ts,.cts,.mts,.tsx,.vue --ignore-path=.gitignore --cache --cache-location=node_modules/.vite/vite-plugin-eslint",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "lint:stylelint": "stylelint \"./**/*.{css,less,scss,sass,vue}\" --fix --allow-empty-input --ignore-path=.gitignore --cache --cache-location=node_modules/.vite/vite-plugin-stylelint",
    "prepare": "is-ci || husky install",
    "release": "release-it",
    "patch:mp-alipay": "tsx ./scripts/patch-mp-alipay.ts",
    "patch:mp-alipay:watch": "nodemon --watch dist/mp-alipay/pages --ext axml --delay 0.5 --exec \"tsx\" ./scripts/patch-mp-alipay.ts",
    "test:unit": "vitest run --coverage"
  },
  "dependencies": {
    "@ctrl/tinycolor": "^3.4.1",
    "@dcloudio/uni-app": "^3.0.0-alpha-3061020221121002",
    "@dcloudio/uni-app-plus": "^3.0.0-alpha-3061020221121002",
    "@dcloudio/uni-components": "^3.0.0-alpha-3061020221121002",
    "@dcloudio/uni-h5": "^3.0.0-alpha-3061020221121002",
    "@dcloudio/uni-mp-alipay": "^3.0.0-alpha-3061020221121002",
    "@dcloudio/uni-mp-baidu": "^3.0.0-alpha-3061020221121002",
    "@dcloudio/uni-mp-kuaishou": "^3.0.0-alpha-3061020221121002",
    "@dcloudio/uni-mp-lark": "^3.0.0-alpha-3061020221121002",
    "@dcloudio/uni-mp-qq": "^3.0.0-alpha-3061020221121002",
    "@dcloudio/uni-mp-toutiao": "^3.0.0-alpha-3061020221121002",
    "@dcloudio/uni-mp-weixin": "^3.0.0-alpha-3061020221121002",
    "@dcloudio/uni-quickapp-webview": "^3.0.0-alpha-3061020221121002",
    "@dcloudio/uni-ui": "^1.4.23",
    "@lukemorales/query-key-factory": "^1.0.3",
    "@tanstack/vue-query": "^4.17.0",
    "@uni-helper/uni-network": "^0.11.1",
    "@uni-helper/uni-use": "^0.9.1",
    "@vueuse/core": "^9.5.0",
    "change-case": "^4.1.2",
    "core-js": "^3.26.1",
    "dayjs": "^1.11.6",
    "lodash-es": "^4.17.21",
    "modern-normalize": "^1.1.0",
    "pinia": "^2.0.25",
    "qs": "^6.11.0",
    "ramda": "^0.28.0",
    "statuses": "^2.0.1",
    "vue": "^3.2.45",
    "vue-i18n": "^9.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/eslint-parser": "^7.19.1",
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@commitlint/prompt": "^17.3.0",
    "@dcloudio/types": "^3.0.19",
    "@dcloudio/uni-automator": "^3.0.0-alpha-3061020221121002",
    "@dcloudio/uni-cli-shared": "^3.0.0-alpha-3061020221121002",
    "@dcloudio/uni-stacktracey": "^3.0.0-alpha-3061020221121002",
    "@dcloudio/vite-plugin-uni": "^3.0.0-alpha-3061020221121002",
    "@iconify/json": "^2.1.141",
    "@mini-types/alipay": "^2.0.15",
    "@modyqyw/fabric": "^5.12.0",
    "@modyqyw/vite-plugin-eslint": "^2.0.10",
    "@release-it/conventional-changelog": "^5.1.1",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.8",
    "@types/lodash-es": "^4.17.6",
    "@types/qs": "^6.9.7",
    "@types/ramda": "^0.28.20",
    "@types/shelljs": "^0.8.11",
    "@types/statuses": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "@uni-helper/uni-app-types": "^0.1.8",
    "@uni-helper/uni-cloud-types": "^0.1.8",
    "@uni-helper/uni-ui-types": "^0.1.6",
    "@uni-helper/vite-plugin-uni-tailwind": "^0.8.1",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitejs/plugin-vue-jsx": "^2.1.1",
    "@vitest/coverage-c8": "^0.25.2",
    "@vitest/ui": "^0.25.2",
    "@vue/test-utils": "^2.2.4",
    "@vue/tsconfig": "^0.1.3",
    "c8": "^7.12.0",
    "commitizen": "^4.2.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.28.0",
    "happy-dom": "^7.7.0",
    "husky": "^8.0.2",
    "is-ci": "^3.0.1",
    "jsdom": "^20.0.3",
    "lint-staged": "^13.0.3",
    "markdownlint-cli": "^0.32.2",
    "miniprogram-api-typings": "^3.6.0",
    "npm-check-updates": "^16.4.1",
    "npm-run-all": "^4.1.5",
    "picocolors": "^1.0.0",
    "pino": "^8.7.0",
    "pino-pretty": "^9.1.1",
    "postcss": "^8.4.19",
    "postcss-preset-env": "^7.8.3",
    "prettier": "^2.7.1",
    "release-it": "^15.5.0",
    "rimraf": "^3.0.2",
    "sass": "^1.56.1",
    "shelljs": "^0.8.5",
    "stylelint": "^14.15.0",
    "tailwind-extensions": "^0.19.2",
    "tailwindcss": "^3.2.4",
    "terser": "^5.15.1",
    "tsx": "^3.12.1",
    "type-fest": "^3.2.0",
    "typescript": "^4.9.3",
    "unocss": "^0.46.5",
    "unplugin-auto-import": "^0.11.4",
    "unplugin-vue-components": "^0.22.9",
    "unplugin-vue-macros": "^0.16.4",
    "vite": "^3.2.4",
    "vite-plugin-inspect": "^0.7.9",
    "vite-plugin-stylelint": "^3.0.8",
    "vitest": "^0.25.2",
    "vue-tsc": "^1.0.9"
  }
}
