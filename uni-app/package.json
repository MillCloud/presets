    "type-fest": "^3.5.1",
    "typescript": "^4.9.4",
    "unocss": "^0.48.3",
    "unplugin-auto-import": "^0.12.1",
    "unplugin-vue-components": "^0.22.12",
    "unplugin-vue-macros": "^1.3.2",
    "vite": "^4.0.4",
    "vite-plugin-inspect": "^0.7.14",
    "vite-plugin-stylelint": "^4.1.6",
    "vitest": "^0.27.0",
    "vue-tsc": "^1.0.24"
  }
}
