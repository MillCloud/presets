{
  "name": "koa",
  "version": "0.0.0",
  "private": true,
  "description": "A boilerplate for koa. ONLY FOR PROTOTYPING!",
  "type": "module",
  "scripts": {
    "check:deps": "taze -f",
    "check:types": "tsc --noEmit",
    "commit": "git-cz",
    "dev": "cross-env NODE_ENV=development nodemon -w src -e .js,.cjs,.mjs,.ts,.cts,.mts,.json --exec tsx ./src/index.ts",
    "lint": "conc \"pnpm:lint:eslint\" \"pnpm:lint:markdownlint\"",
    "lint:eslint": "eslint . --fix --cache --ignore-path=.gitignore",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "prepare": "is-ci || simple-git-hooks"
  },
  "dependencies": {
    "@faker-js/faker": "^8.0.2",
    "@koa/cors": "^4.0.0",
    "@koa/router": "^12.0.0",
    "@modyqyw/utils": "^8.0.0",
    "formidable": "^2.1.2",
    "koa": "^2.14.1",
    "koa-bodyparser": "^4.4.1",
    "koa-mount": "^4.0.0",
    "koa-pino-logger": "^4.0.0",
    "koa-sslify": "^5.0.1",
    "koa-static": "^5.0.0",
    "koa-zod-router": "^2.0.1",
    "pino": "^8.15.0",
    "pino-pretty": "^10.2.0"
  },
  "devDependencies": {
    "@modyqyw/fabric": "^9.0.2",
    "@tsconfig/node18": "^18.2.0",
    "@types/formidable": "^2.0.6",
    "@types/koa": "^2.13.8",
    "@types/koa-bodyparser": "^4.3.10",
    "@types/koa-mount": "^4.0.2",
    "@types/koa-pino-logger": "^3.0.1",
    "@types/koa-sslify": "^4.0.3",
    "@types/koa-static": "^4.0.2",
    "@types/koa__cors": "^4.0.0",
    "@types/koa__router": "^12.0.0",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "commitizen": "^4.3.0",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.46.0",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.2.3",
    "markdownlint-cli": "^0.35.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.1",
    "simple-git": "^3.19.1",
    "simple-git-hooks": "^2.9.0",
    "sort-package-json": "^2.5.1",
    "taze": "^0.11.2",
    "tsx": "^3.12.7",
    "type-fest": "^4.2.0",
    "typescript": "^5.1.6"
  },
  "packageManager": "pnpm@8.6.12",
  "engines": {
    "node": "18"
  }
}
