{
  "name": "koa",
  "version": "0.0.0",
  "private": true,
  "description": "A boilerplate for koa. ONLY FOR PROTOTYPING!",
  "type": "module",
  "scripts": {
    "dev": "nodemon -w src -e .js,.cjs,.mjs,.ts,.cts,.mts,.json --exec tsx ./src/index.ts",
    "start": "pnpm run dev",
    "commit": "git-cz",
    "check:deps": "ncu --dep dev,optional,prod,packageManager",
    "check:types": "tsc --noEmit",
    "lint": "run-p lint:eslint lint:markdownlint check:types",
    "lint:eslint": "eslint . --ext=.js,.cjs,.mjs,.ts,.cts,.mts --fix --cache --no-error-on-unmatched-pattern --ignore-path=.gitignore",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "prepare": "is-ci || husky install"
  },
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "@koa/cors": "^4.0.0",
    "@koa/router": "^12.0.0",
    "koa": "^2.14.1",
    "koa-body": "^6.0.1",
    "koa-joi-router": "^8.0.0",
    "koa-mount": "^4.0.0",
    "koa-pino-logger": "^4.0.0",
    "koa-sslify": "^5.0.1",
    "koa-static": "^5.0.0",
    "lodash-es": "^4.17.21",
    "pino": "^8.8.0",
    "pino-pretty": "^9.1.1",
    "ramda": "^0.28.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/eslint-parser": "^7.19.1",
    "@modyqyw/fabric": "^5.16.0",
    "@tsconfig/node18": "^1.0.1",
    "@types/koa": "^2.13.5",
    "@types/koa-joi-router": "^8.0.4",
    "@types/koa-mount": "^4.0.2",
    "@types/koa-pino-logger": "^3.0.1",
    "@types/koa-sslify": "^4.0.3",
    "@types/koa-static": "^4.0.2",
    "@types/koa__cors": "^3.3.0",
    "@types/koa__router": "^12.0.0",
    "@types/lodash-es": "^4.17.6",
    "@types/ramda": "^0.28.20",
    "@types/shelljs": "^0.8.11",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "commitizen": "^4.2.6",
    "cross-env": "^7.0.3",
    "eslint": "^8.31.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.1.0",
    "markdownlint-cli": "^0.33.0",
    "nodemon": "^2.0.20",
    "npm-check-updates": "^16.6.2",
    "npm-run-all": "^4.1.5",
    "picocolors": "^1.0.0",
    "prettier": "^2.8.2",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.5",
    "tsx": "^3.12.1",
    "type-fest": "^3.5.1",
    "typescript": "^4.9.4"
  }
}
