{
  "name": "koa",
  "version": "0.0.0",
  "private": true,
  "description": "A boilerplate for koa. ONLY FOR PROTOTYPING!",
  "type": "module",
  "scripts": {
    "dev": "nodemon -w src -e .js,.cjs,.mjs,.ts,.cts,.mts,.json --exec tsx ./src/index.ts",
    "commit": "git-cz",
    "check:deps": "taze",
    "check:types": "tsc --noEmit",
    "lint": "run-p lint:eslint lint:markdownlint check:types",
    "lint:eslint": "eslint . --fix --cache --ignore-path=.gitignore",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "prepare": "is-ci || husky install"
  },
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "@koa/cors": "^4.0.0",
    "@koa/router": "^12.0.0",
    "@modyqyw/utils": "^5.0.0",
    "formidable": "^2.1.1",
    "koa": "^2.14.1",
    "koa-bodyparser": "^4.4.0",
    "koa-mount": "^4.0.0",
    "koa-pino-logger": "^4.0.0",
    "koa-sslify": "^5.0.1",
    "koa-static": "^5.0.0",
    "koa-zod-router": "^1.1.3",
    "pino": "^8.11.0",
    "pino-pretty": "^10.0.0"
  },
  "devDependencies": {
    "@modyqyw/fabric": "^7.5.0",
    "@tsconfig/node18": "^1.0.1",
    "@types/formidable": "^2.0.5",
    "@types/koa": "^2.13.5",
    "@types/koa-bodyparser": "^4.3.10",
    "@types/koa-mount": "^4.0.2",
    "@types/koa-pino-logger": "^3.0.1",
    "@types/koa-sslify": "^4.0.3",
    "@types/koa-static": "^4.0.2",
    "@types/koa__cors": "^4.0.0",
    "@types/koa__router": "^12.0.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "commitizen": "^4.3.0",
    "eslint": "^8.36.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.2.0",
    "markdownlint-cli": "^0.33.0",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.6",
    "sort-package-json": "^2.4.1",
    "taze": "^0.9.1",
    "tsx": "^3.12.6",
    "type-fest": "^3.6.1",
    "typescript": "^5.0.2"
  }
}
