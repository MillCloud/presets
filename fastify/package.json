{
  "name": "fastify",
  "version": "0.0.0",
  "private": true,
  "description": "A boilerplate for fastify. ONLY FOR PROTOTYING!",
  "type": "module",
  "scripts": {
    "dev": "nodemon -w src -e .js,.cjs,.mjs,.ts,.cts,.mts,.json --exec tsx ./src/index.ts",
    "start": "run-s dev",
    "commit": "cz",
    "check:deps": "ncu",
    "check:types": "tsc --noEmit",
    "lint": "run-p lint:eslint lint:markdownlint check:types",
    "lint:eslint": "eslint . --fix --ext=.js,.cjs,.mjs,.jsx,.ts,.cts,.mts,.tsx,.vue --ignore-path=.gitignore --cache",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "prepare": "is-ci || husky install"
  },
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "@fastify/autoload": "^5.6.0",
    "@fastify/cors": "^8.2.0",
    "@fastify/error": "^3.1.0",
    "@fastify/formbody": "^7.4.0",
    "@fastify/multipart": "^7.3.0",
    "@fastify/sensible": "^5.2.0",
    "@fastify/static": "^6.6.0",
    "fastify": "^4.10.2",
    "fastify-cli": "^5.7.0",
    "fastify-plugin": "^4.4.0",
    "lodash-es": "^4.17.21",
    "pino": "^8.7.0",
    "pino-pretty": "^9.1.1",
    "ramda": "^0.28.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/eslint-parser": "^7.19.1",
    "@modyqyw/fabric": "^5.13.0",
    "@tsconfig/node18": "^1.0.1",
    "@types/lodash-es": "^4.17.6",
    "@types/ramda": "^0.28.20",
    "@types/shelljs": "^0.8.11",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "commitizen": "^4.2.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.29.0",
    "husky": "^8.0.2",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.1.0",
    "markdownlint-cli": "^0.32.2",
    "nodemon": "^2.0.20",
    "npm-check-updates": "^16.4.3",
    "npm-run-all": "^4.1.5",
    "picocolors": "^1.0.0",
    "prettier": "^2.8.0",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.5",
    "tsx": "^3.12.1",
    "type-fest": "^3.3.0",
    "typescript": "^4.9.3"
  }
}
