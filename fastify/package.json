{
  "name": "fastify",
  "version": "0.0.0",
  "private": true,
  "description": "A boilerplate for fastify. ONLY FOR PROTOTYING!",
  "type": "module",
  "scripts": {
    "dev": "nodemon -w src -e .js,.cjs,.mjs,.ts,.cts,.mts,.json --exec tsx ./src/index.ts",
    "start": "pnpm run dev",
    "commit": "git-cz",
    "check:deps": "ncu --dep dev,optional,prod,packageManager",
    "check:types": "tsc --noEmit",
    "lint": "run-p lint:eslint lint:markdownlint check:types",
    "lint:eslint": "eslint . --fix --cache --no-error-on-unmatched-pattern",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "prepare": "is-ci || husky install"
  },
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "@fastify/autoload": "^5.7.1",
    "@fastify/cors": "^8.2.0",
    "@fastify/error": "^3.2.0",
    "@fastify/formbody": "^7.4.0",
    "@fastify/multipart": "^7.4.0",
    "@fastify/sensible": "^5.2.0",
    "@fastify/static": "^6.9.0",
    "@modyqyw/utils": "^4.1.0",
    "fastify": "^4.13.0",
    "fastify-cli": "^5.7.1",
    "fastify-plugin": "^4.5.0",
    "pino": "^8.10.0",
    "pino-pretty": "^9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/eslint-parser": "^7.19.1",
    "@modyqyw/fabric": "^7.0.1",
    "@tsconfig/node18": "^1.0.1",
    "@types/shelljs": "^0.8.11",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.34.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.1.1",
    "markdownlint-cli": "^0.33.0",
    "nodemon": "^2.0.20",
    "npm-check-updates": "^16.7.4",
    "npm-run-all": "^4.1.5",
    "picocolors": "^1.0.0",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "shelljs": "^0.8.5",
    "tsx": "^3.12.3",
    "type-fest": "^3.5.7",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">= 14.18"
  }
}
