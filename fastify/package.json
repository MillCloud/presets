{
  "name": "fastify",
  "version": "0.0.0",
  "private": true,
  "description": "A boilerplate for fastify. ONLY FOR PROTOTYING!",
  "type": "module",
  "scripts": {
    "dev": "nodemon -w src -e .js,.cjs,.mjs,.ts,.cts,.mts,.json --exec tsx ./src/index.ts",
    "commit": "git-cz",
    "check:deps": "ncu --dep dev,optional,prod,packageManager",
    "check:types": "tsc --noEmit",
    "lint": "run-p lint:eslint lint:markdownlint check:types",
    "lint:eslint": "eslint . --fix --cache --ignore-path=.gitignore",
    "lint:markdownlint": "markdownlint . --fix --ignore=CHANGELOG.md --ignore-path=.gitignore",
    "prepare": "is-ci || husky install"
  },
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "@fastify/autoload": "^5.7.1",
    "@fastify/cors": "^8.2.1",
    "@fastify/error": "^3.2.0",
    "@fastify/formbody": "^7.4.0",
    "@fastify/multipart": "^7.5.0",
    "@fastify/sensible": "^5.2.0",
    "@fastify/static": "^6.9.0",
    "@modyqyw/utils": "^5.0.0",
    "fastify": "^4.14.1",
    "fastify-cli": "^5.7.1",
    "fastify-plugin": "^4.5.0",
    "pino": "^8.11.0",
    "pino-pretty": "^10.0.0"
  },
  "devDependencies": {
    "@modyqyw/fabric": "^7.4.1",
    "@tsconfig/node18": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "commitizen": "^4.3.0",
    "eslint": "^8.36.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.2.0",
    "markdownlint-cli": "^0.33.0",
    "nodemon": "^2.0.21",
    "npm-check-updates": "^16.7.12",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "sort-package-json": "^2.4.1",
    "tsx": "^3.12.5",
    "type-fest": "^3.6.1",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">= 14.18"
  }
}
